{
  "name": "node-webcrypto-p11",
  "version": "2.0.2",
  "description": "A WebCrypto Polyfill built on PKCS11",
  "main": "build/index.js",
  "types": "index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "test": "mocha",
    "test:softhsm": "mocha",
    "test:nss": "PV_CRYPTO=nss mocha",
    "build": "tsc",
    "lint": "tslint 'src/**/*.ts'",
    "prepub": "npm run lint && npm run build",
    "pub": "npm version patch && npm publish && git push",
    "coverage": "nyc npm test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PeculiarVentures/node-webcrypto-p11.git"
  },
  "dependencies": {
    "asn1js": "^2.0.21",
    "graphene-pk11": "^2.1.1",
    "pkijs": "^2.1.74",
    "tslib": "^1.9.3",
    "webcrypto-core": "^1.0.4"
  },
  "keywords": [
    "crypto",
    "graphene",
    "webcrypto",
    "pkcs11",
    "rsa",
    "aes",
    "ec",
    "digest",
    "secp256k1",
    "nss",
    "x25519"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PeculiarVentures/node-webcrypto-p11/issues"
  },
  "homepage": "https://github.com/PeculiarVentures/node-webcrypto-p11#readme",
  "devDependencies": {
    "@types/asn1js": "0.0.1",
    "@types/node": "^10.12.24",
    "babel-polyfill": "^6.26.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.2.0",
    "pvtsutils": "^1.0.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ]
  }
}
